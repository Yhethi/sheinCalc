{
    "version": 2,
    "builds": [
      { "src": "api/app.py", "use": "@vercel/python" },
      { "src": "vite.config.js", "use": "@vercel/static-build", "config": { "distDir": "dist" } }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "/api/app.py" },
      { "src": "/(.*)", "dest": "/dist/$1" }
    ]
  }